# Aliases
alias sl="ls"
alias ls="ls -CF"
alias ll="ls -lhA"

alias ..="cd .."
alias fhere="find . -name "

alias cat='ccat'

alias psg="ps aux | grep -v grep | grep -i -e VSZ -e"
alias df="df -Tha --total"
alias du="du -ach | sort -h"
alias free="free -mt"
alias ps="ps auxf"

alias mkdir="mkdir -pv"
alias wget="wget -c"
alias histg="history | grep"

